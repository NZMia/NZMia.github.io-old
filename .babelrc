{
  "presets":[
    ["env", {
      "modules": false,
      "targets": {"browsers": ["last 2 versions","safari >= 7"]}
    }],
    "react",
    "stage-0"

  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": true},
      "ant"],

    ["import", {
      "libraryName": "ant-mobile",
      "libraryDirectory": "lib",
      "style": true},
      "ant-mobile"]
  ]
}
